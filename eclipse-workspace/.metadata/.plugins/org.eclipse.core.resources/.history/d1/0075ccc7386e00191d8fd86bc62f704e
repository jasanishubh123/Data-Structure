#include <stdio.h>
#include <stdlib.h>

int main(void) {
	printf("Integer Values\n");
	printf("%d %d %d" ,32767,32767+1,32767+10);
	printf("\n");
	printf("Long Integer Values\n");
	printf("%ld %ld %ld" ,32767L,32767L+1L,32767L+10L);
	return EXIT_SUCCESS;
}

#include <stdio.h>
#include <stdlib.h>

main( )
{
int i = -3, j = 3 ;
printf("%d\n",!i);
if ( !i + !j * 1 )
printf ( "\nMassaro" ) ;
else
printf ( "\nBennarivo" ) ;
}

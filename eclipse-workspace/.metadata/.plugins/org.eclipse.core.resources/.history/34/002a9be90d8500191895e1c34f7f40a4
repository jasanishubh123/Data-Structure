#include <stdio.h>
#include <stdlib.h>
main( )
{
int kk = 65 ,ll ;
ll = ( kk == 65 ? printf ( "\n kk is equal to 65" ) : printf ( "\n kk is not
equal to 65" ) ) ;
printf( "%d", ll ) ;
}
